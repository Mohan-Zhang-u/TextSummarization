{
    "abstractor": {
        "net": "base_abstractor",
        "net_args": {
            "vocab_size": 30004,
            "n_hidden": 256,
            "emb_dim": 128,
            "n_layer": 1,
            "bidirectional": true
        },
        "traing_params": {
            "clip_grad_norm": 2.0,
            "optimizer": [
                "adam",
                {
                    "lr": 0.001
                }
            ],
            "lr_decay": 0.5,
            "batch_size": 32
        }
    },
    "extractor": {
        "net": "ml_rnn_extractor",
        "net_args": {
            "vocab_size": 30004,
            "lstm_hidden": 256,
            "lstm_layer": 1,
            "emb_dim": 128,
            "conv_hidden": 100,
            "bidirectional": true
        },
        "traing_params": {
            "clip_grad_norm": 2.0,
            "optimizer": [
                "adam",
                {
                    "lr": 0.001
                }
            ],
            "lr_decay": 0.5,
            "batch_size": 32
        }
    },
    "rl": true,
    "split": "test",
    "beam": 5,
    "diverse": 1.0
}